{
  "manifest_version": 2,
  "name": "GitHub Pull Request Tab",
  "version": "1.1",
  "description": "Create PullRequest tabs quickly",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "background" : {
    "persistent": false,
    "scripts": [
      "js/lib/jquery-2.1.0.min.js",
      "js/popup.js",
      "js/menu.js"
    ]
  },
  "icons" : {
    "16": "icon16.png",
    "128": "icon.png"
  }
}